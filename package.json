{
  "name": "semantic-grounding",
  "version": "0.0.0",
  "description": "semantic-grounding",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "python app/app.py",
    "pytest": "python -m unittest discover -v",
    "compile-app": "browserify src/* -p [ tsify --noImplicitAny ] -x=babylonjs -x=cannon -x=earcut -x=oimo -x=socket.io-client > app.js",
    "compile-vendor": "browserify -r babylonjs -r cannon -r earcut -r oimo -r socket.io-client > vendor.js",
    "serve": "node server.js"
  },
  "dependencies": {
    "@types/socket.io-client": "^1.4.32",
    "babylonjs": "^3.2.0-rc.3",
    "browserify": "^16.2.0",
    "cannon": "^0.6.2",
    "earcut": "^2.1.3",
    "npm": "^6.1.0",
    "oimo": "^1.0.9",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^8.0.0",
    "coveralls": "^2.0.0",
    "jest": "^22.0.4",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.1",
    "tsify": "^4.0.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
